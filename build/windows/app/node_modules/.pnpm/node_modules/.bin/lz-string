#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/a/sample-fe/sample-fe/build/windows/app/node_modules/.pnpm/node_modules/lz-string/bin/node_modules:/mnt/d/a/sample-fe/sample-fe/build/windows/app/node_modules/.pnpm/node_modules/lz-string/node_modules:/mnt/d/a/sample-fe/sample-fe/build/windows/app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/a/sample-fe/sample-fe/build/windows/app/node_modules/.pnpm/node_modules/lz-string/bin/node_modules:/mnt/d/a/sample-fe/sample-fe/build/windows/app/node_modules/.pnpm/node_modules/lz-string/node_modules:/mnt/d/a/sample-fe/sample-fe/build/windows/app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lz-string/bin/bin.js" "$@"
else
  exec node  "$basedir/../lz-string/bin/bin.js" "$@"
fi
